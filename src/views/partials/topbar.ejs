    <div id="calculadora" style="display: none; opacity: 0.91">
      <div
        id="calculadoraheader texto-right"
        style="text-align: right !important"
      >
        <button class="cerrarcalculadora" onclick="cerrarcalculadora()">
          X
        </button>
      </div>
      <div class="mycalc">
        <form name="calculator" id="calculator">
          <input
            type="text"
            class="inputcalculadora"
            name="inputwindow"
            id="inputwindow"
          />
          <div>
            <input
              class="botoncalculadora"
              type="button"
              id="one"
              value="1"
              onclick="takeValue(document.getElementById('one').value)"
            />
            <input
              class="botoncalculadora"
              type="button"
              id="two"
              value="2"
              onclick="takeValue(document.getElementById('two').value)"
            />
            <input
              class="botoncalculadora"
              type="button"
              id="three"
              value="3"
              onclick="takeValue(document.getElementById('three').value)"
            />
            <input
              class="botoncalculadora"
              type="button"
              id="plus"
              value="+"
              onclick="takeValue(document.getElementById('plus').value)"
            />
          </div>
          <div>
            <input
              class="botoncalculadora"
              type="button"
              id="four"
              value="4"
              onclick="takeValue(document.getElementById('four').value)"
            />
            <input
              class="botoncalculadora"
              type="button"
              id="five"
              value="5"
              onclick="takeValue(document.getElementById('five').value)"
            />
            <input
              class="botoncalculadora"
              type="button"
              id="six"
              value="6"
              onclick="takeValue(document.getElementById('six').value)"
            />
            <input
              class="botoncalculadora"
              type="button"
              id="minus"
              value="-"
              onclick="takeValue(document.getElementById('minus').value)"
            />
          </div>
          <div>
            <input
              class="botoncalculadora"
              type="button"
              id="seven"
              value="7"
              onclick="takeValue(document.getElementById('seven').value)"
            />
            <input
              class="botoncalculadora"
              type="button"
              id="eight"
              value="8"
              onclick="takeValue(document.getElementById('eight').value)"
            />
            <input
              class="botoncalculadora"
              type="button"
              id="nine"
              value="9"
              onclick="takeValue(document.getElementById('nine').value)"
            />
            <input
              class="botoncalculadora"
              type="button"
              id="by"
              value="*"
              onclick="takeValue(document.getElementById('by').value)"
            />
          </div>
          <div>
            <input
              class="botoncalculadora"
              type="button"
              id="zero"
              value="0"
              onclick="takeValue(document.getElementById('zero').value)"
            />
            <input
              class="botoncalculadora"
              type="button"
              id="decimal"
              value="."
              onclick="takeValue(document.getElementById('decimal').value)"
            />
            <input
              class="botoncalculadora"
              type="button"
              id="devide"
              value="/"
              onclick="takeValue(document.getElementById('devide').value)"
            />
            <input
              class="botoncalculadora"
              type="button"
              id="percent"
              value="%"
              onclick="takeValue(document.getElementById('percent').value)"
            />
          </div>
          <div class="results">
            <button
              class="botoncalculadora"
              type="button"
              id="clear"
              value=""
              onclick="clearInput(document.getElementById('clear').value)"
            >
              Limpiar
            </button>
            <input
              class="botoncalculadora"
              type="button"
              id="equal"
              value="="
              onclick="calculateResult()"
            />
          </div>
        </form>
      </div>
    </div>

    <li class="notification-list">
